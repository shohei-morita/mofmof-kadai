<h1>物件一覧</h1>

<table>
  <% @properties.each do |property| %>
  <tr>
    <th>物件名</th>
    <th>賃料</th>
    <th>住所</th>
    <th>築年数</th>
    <th>備考</th>
  </tr>
  <tr>
    <td><%= property.name %></td>
    <td><%= property.cost %></td>
    <td><%= property.address %></td>
    <td><%= property.age %></td>
    <td><%= property.note %></td>
  </tr>
  <% end %>
</table>
